<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scalior.android.schedulealarmmanager.sampleapp" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_sam_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.scalior.android.schedulealarmmanager.sampleapp.ScheduleTestActivity"
            android:label="@string/title_activity_schedule_test" >
        </activity>
        <activity
            android:name="com.scalior.android.schedulealarmmanager.sampleapp.WeeklySchedule"
            android:label="@string/title_activity_weekly_schedule" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
